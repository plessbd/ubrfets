{
	"env":{
		"browser": true
	},
	"globals": {
		"PDFJS": false,
		"localforage": false,
		"moment": false,
		"setPdfField": false,
		"renderTimesheet": false,
		"observedHolidays": false,
		"userOperations": false
	},
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"rules": {
		"curly": [
			2,
			"all"
		],
		"guard-for-in": 2,
		"no-extend-native": 2,
		"block-scoped-var": 0,
		"no-use-before-define": 2,
		"no-caller": 2,
		"no-sequences": 2,
		"no-irregular-whitespace": 2,
		"no-new": 2,
		"no-extra-parens": 0,
		"no-undef": 2,
		"no-unused-vars": 2,
		"dot-notation": [
			1, {
				"allowKeywords": true,
				"allowPattern": "^[a-z]+(_[a-z]+)+$"
			}
		],
		"no-empty": 2,
		"no-implicit-coercion": [
			2, {
				"boolean": true,
				"number": true,
				"string": true
			}
		],
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"key-spacing": [
			2, {
				"beforeColon": false,
				"afterColon": true
			}
		],
		"comma-spacing": [
			2, {
				"before": false,
				"after": true
			}
		],
		"space-unary-ops": 0,
		"semi-spacing": [
			2, {
				"before": false,
				"after": true
			}
		],
		"no-spaced-func": 2,
		"comma-dangle": [
			2,
			"never"
		],
		"no-trailing-spaces": 2,
		"eol-last": 2,
		"semi": [
			2,
			"always"
		],
		"space-infix-ops": 2,
		"consistent-this": [
			1,
			"self"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"quotes": [2, "double"]
	}
}
